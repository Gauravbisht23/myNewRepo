<aura:component >
    <aura:attribute name="a1" type="Integer" default="2" />
    <aura:attribute name="a2" type="Integer" default="30" />
    <aura:attribute name="multiply" type="Integer" />
    
    {!v.a1}*{!v.a2}={!v.multiply}
    <br/><br/>
    <ui:button label="Equals To" press="{!c.mul}" />
</aura:component>